{
    "name": "search",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://thoth-station.github.io/search",
    "dependencies": {
        "@emotion/react": "^11.4.0",
        "@emotion/styled": "^11.3.0",
        "@material/material-color-utilities": "^0.1.2",
        "@mui/icons-material": "^5.0.0",
        "@mui/lab": "^5.0.0-alpha.67",
        "@mui/material": "^5.4.0",
        "@mui/styles": "^5.4.2",
        "@mui/x-data-grid": "^5.5.0",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.18",
        "@types/react": "^17.0.39",
        "@types/react-dom": "^17.0.11",
        "axios": "^0.21.1",
        "gh-pages": "^3.2.0",
        "husky": "^7.0.4",
        "js-yaml": "^4.1.0",
        "json": "^11.0.0",
        "lint-staged": "^12.1.2",
        "prettier": "^2.5.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^3.1.4",
        "react-query": "^3.33.5",
        "react-router-dom": "^6.0.0-beta.8",
        "react-scripts": "4.0.3",
        "react-styleguidist": "^11.1.7",
        "tsc-files": "^1.1.3",
        "typescript": "^4.5.5",
        "vis-data": "^7.1.2",
        "vis-network": "^9.0.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --passWithNoTests",
        "eject": "react-scripts eject",
        "predeploy": "yarn run build",
        "deploy": "gh-pages -d build",
        "generate": "rc-autocomplete",
        "styleguide": "styleguidist server",
        "styleguide:build": "styleguidist build && rm -rf docs && mv styleguide docs",
        "lint": "eslint --ignore-path .eslintignore --ext .js,.ts src --color",
        "prettier": "prettier src -c",
        "lint-staged": "lint-staged --config lint-staged.js",
        "husky-install": "husky install",
        "dev": "tsc --watch",
        "pull-schema": "openapi-typescript https://raw.githubusercontent.com/thoth-station/user-api/master/openapi/openapi.yaml --output src/lib/schema.ts && prettier src/lib/schema.ts -w"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "jest": {
        "transformIgnorePatterns": [
            "node_modules/?!(vis-network)"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/color": "^3.0.3",
        "@types/js-yaml": "^4.0.5",
        "@typescript-eslint/eslint-plugin": "^5.15.0",
        "@typescript-eslint/parser": "^5.15.0",
        "color": "^4.2.3",
        "core-js": "^3.20.3",
        "eslint-plugin-jest": "^26.0.0",
        "openapi-typescript": "^5.4.0",
        "prop-types": "^15.8.1",
        "typedoc": "^0.22.13"
    }
}
